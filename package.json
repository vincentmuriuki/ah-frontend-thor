{
  "name": "ah-frontend-thor",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "NODE_ENV=production env-cmd .env webpack",
    "start:dev": "NODE_ENV=development env-cmd .env webpack-dev-server --open",
    "test": "NODE_ENV=testing jest --coverage",
    "test:dev": "yarn test --watch",
    "coverage": "yarn test --coverage",
    "lint": "github-lint",
    "heroku-prebuild": "yarn install",
    "heroku-postbuild": "yarn build"
  },
  "dependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "dotenv": "^6.1.0",
    "dotenv-webpack": "^1.5.7",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-github": "^1.6.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "expect": "^23.6.0",
    "fetch-mock": "^7.2.5",
    "file-loader": "^2.0.0",
    "flow-bin": "^0.87.0",
    "graphql": "^14.0.2",
    "html-webpack-plugin": "^3.2.0",
    "jest-fetch-mock": "^2.0.1",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-facebook-login": "^4.1.1",
    "react-google-login": "^4.0.0",
    "react-redux": "^5.1.1",
    "react-router-dom": "^4.3.1",
    "react-tagsinput": "^3.19.0",
    "react-test-renderer": "^16.6.3",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.7",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "^2.3.0",
    "sinon": "^7.2.2",
    "sweetalert2": "^7.32.2",
    "toastr": "^2.1.4",
    "webpack": "^4.26.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "devDependencies": {
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-polyfill": "^6.26.0",
    "css-loader": "^1.0.1",
    "env-cmd": "^8.0.2",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "eslint": "^5.9.0",
    "faker": "^4.1.0",
    "fetch-mock": "^7.2.5",
    "jest": "^23.6.0",
    "node-sass": "^4.10.0",
    "puppeteer": "^1.10.0",
    "react-router": "^4.3.1",
    "redux-mock-store": "^1.5.3",
    "regenerator-runtime": "^0.13.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/tests/setupTests.js",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.jsx",
      "!src/reducers/rootReducer.js"
    ],
    "coverageReporters": [
      "lcov",
      "json",
      "html"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/__mocks__/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/tests/__mocks__/styleMock.js"
    }
  }
